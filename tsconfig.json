{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "resolveJsonModule": true,
    "paths": {
      "@prisma": [
        "prisma/generated/prisma/client"
      ],
      "@database/*": [
        "src/database/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@constants/*": [
        "src/common/constants/*"
      ],
      "@decorators/*": [
        "src/common/decorators/*"
      ],
      "@exceptions/*": [
        "src/common/exceptions/*"
      ],
      "@filters/*": [
        "src/common/filters/*"
      ],
      "@guards/*": [
        "src/common/guards/*"
      ],
      "@interceptors/*": [
        "src/common/interceptors/*"
      ],
      "@models/*": [
        "src/common/models/*"
      ],
      "@pipes/*": [
        "src/common/pipes/*"
      ],
      "@services/*": [
        "src/common/services/*"
      ],
      "@middlewares/*": [
        "src/common/middlewares/*"
      ],
      "@views/*": [
        "src/common/views/*"
      ],
      "@templates/*": [
        "src/modules/mail/templates/*"
      ]
    }
  }
}
